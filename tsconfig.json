{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "types": [
            "@cloudflare/workers-types"
        ],
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./src/*"
            ]
        }
    },
    "include": [
        ".astro/types.d.ts",
        "**/*"
    ],
    "exclude": [
        "dist"
    ]
}